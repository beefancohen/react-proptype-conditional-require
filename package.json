{
  "name": "react-proptype-conditional-require",
  "version": "0.1.7",
  "description": "Conditionally require a React proptype based on other props and/or other conditions.",
  "main": "dist/react-proptype-conditional-require.js",
  "scripts": {
    "lint": "eslint --config .eslintrc test/isRequiredIf.js isRequiredIf.js",
    "pretest": "rollup -c rollup.config.test.js",
    "test": "tape build/test-bundle.js | faucet",
    "ci:test": "npm run pretest && npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "test:coverage": "nyc --reporter=lcov --reporter=text-lcov tape build/test-bundle.js",
    "ci:build": "npm run lint && npm run ci:test",
    "prebuild": "npm run lint && npm run test",
    "build": "rollup -c rollup.config.cjs.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evcohen/react-proptype-conditional-require"
  },
  "keywords": [
    "react",
    "proptypes",
    "require",
    "isRequired",
    "isRequiredIf",
    "conditional",
    "props"
  ],
  "author": "Ethan Cohen",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.7"
  },
  "devDependencies": {
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "faucet": "0.0.1",
    "nyc": "^5.5.0",
    "rollup": "^0.25.2",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-multi-entry": "^1.1.0",
    "source-map-support": "^0.4.0",
    "tape": "^4.4.0"
  }
}
