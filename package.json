{
  "name": "react-proptype-conditional-require",
  "version": "0.1.6",
  "description": "Conditionally require a React proptype based on other props and/or other conditions.",
  "main": "dist/react-proptype-conditional-require.js",
  "scripts": {
    "lint": "eslint --config .eslintrc test/isRequiredIf.js isRequiredIf.js",
    "pretest": "rollup -c ./config/rollup.config.test.js",
    "test": "tap -c --cov -R=classic --coverage-report=text-lcov test/bundle.js --timeout=60",
    "ci:build": "npm run test && npm run build",
    "prebuild": "npm run lint",
    "build": "rollup -c ./config/rollup.config.cjs.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evcohen/react-proptype-conditional-require"
  },
  "keywords": [
    "react",
    "proptypes",
    "require",
    "isRequired",
    "isRequiredIf",
    "conditional",
    "props"
  ],
  "author": "Ethan Cohen",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.7"
  },
  "devDependencies": {
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "rollup": "^0.25.2",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-multi-entry": "^1.1.0",
    "source-map-support": "^0.4.0",
    "tap": "^5.4.2",
    "tape": "^4.4.0"
  }
}
