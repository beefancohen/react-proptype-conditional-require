{
  "name": "react-proptype-conditional-require",
  "version": "1.0.2",
  "description": "Conditionally require a React proptype based on other props and/or other conditions.",
  "main": "dist/react-proptype-conditional-require.js",
  "scripts": {
    "clean": "rimraf coverage .nyc_output npm-debug.log",
    "lint": "eslint --config .eslintrc __tests__/**/*-test.js isRequiredIf.js",
    "test": "jest --coverage",
    "posttest": "npm run clean",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "rollup -c ./config/rollup.config.cjs.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evcohen/react-proptype-conditional-require"
  },
  "keywords": [
    "react",
    "proptypes",
    "require",
    "isRequired",
    "isRequiredIf",
    "conditional",
    "props"
  ],
  "author": "Ethan Cohen",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-jest": "^17.0.2",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-multi-entry": "^2.0.1",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "reports"
  }
}
